{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": 0,
        "no-underscore-dangle": 0,
        "import/no-named-as-default": 0,
        "max-len": 0
    }
}
